{
  "name": "vue3-mall-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "stylelint": "stylelint ./**/*.{css,less,vue,html} --fix",
    "prettier": "prettier --write ./**/*.{html,vue,ts,cjs,json,md}",
    "format": "pnpm run eslint && pnpm run stylelint && pnpm run prettier"
  },
  "dependencies": {
    "@types/node": "^20.11.24",
    "axios": "^1.6.7",
    "element-plus": "^2.6.0",
    "pinia": "^2.1.7",
    "stylelint": "^16.2.1",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.2.5",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^3.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
